<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <!--
      Base application theme, dependent on API level. This theme is replaced
      by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
  -->

  <style name="Base.TradeHero"
      parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorAccent">@color/text_secondary_accented</item>
    <item name="colorPrimary">@color/tradehero_blue</item>
    <item name="colorPrimaryDark">#ff0b4a9e</item>
    <item name="colorControlNormal">@color/text_secondary</item>
    <item name="colorControlHighlight">#b7b7b7</item>
    <item name="android:textColorPrimary">@color/primary_text</item>
    <item name="android:textColorPrimaryInverse">@color/primary_inverse_text</item>
    <item name="android:textColorSecondary">@color/secondary_text</item>
    <item name="android:textColorSecondaryInverse">@color/secondary_inverse_text</item>
    <item name="android:textColorHighlight">@color/accented_primary_text</item>
    <item name="android:textColorHighlightInverse">@color/accented_secondary_text</item>
    <item name="slidingTabBackground">@color/tradehero_dark_blue</item>
  </style>

  <style name="TradeHero"
      parent="Base.TradeHero">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowActionBarOverlay">false</item>
  </style>

  <style name="TradeHero.Splash">
    <item name="android:windowBackground">@drawable/splash_screen_theme</item>
  </style>

  <style name="TradeHero.ActionBar" parent="Widget.AppCompat.Toolbar">
    <item name="android:background">?attr/colorPrimary</item>
    <item name="theme">@style/ThemeOverlay.ActionBar.TradeHero</item>
    <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
  </style>

  <style name="ThemeOverlay.ActionBar.TradeHero" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
    <item name="android:windowActionBarOverlay">true</item>
    <!-- Support library compatibility -->
    <item name="windowActionBarOverlay">true</item>
  </style>

  <style name="TradeHero.Settings"
      parent="TradeHero">
  </style>

  <style name="AppBaseTheme.NoActionBar"
      parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:background">@color/white</item>
  </style>

  <style name="TH"/>

  <style name="TH.Btn_Bar_Base"
      parent="TH.Button">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TH.Btn_Buy_Sell"
      parent="TH.Btn_Bar_Base">
    <item name="android:background">@drawable/blue_rounded_button_selector</item>
    <item name="android:textColor">@color/text_primary_inverse</item>
  </style>

  <style name="TH.FX_CLOSE"
      parent="TH.Btn_Buy_Sell">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">0</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:minWidth">@dimen/size_10</item>
    <item name="android:background">@drawable/red_rounded_button_selector</item>
  </style>

  <style name="TH.Btn_Time_Span"
      parent="TH.Btn_Bar_Base">
    <item name="android:textColor">@color/text_secondary</item>
    <item name="android:textSize">@dimen/text_caption</item>
    <item name="android:minHeight">@dimen/timespan_button_min_height</item>
  </style>

  <style name="TH.BtnQuickPriceSet"
      parent="TH.Btn_Bar_Base">
    <item name="android:textColor">@color/text_secondary</item>
    <item name="android:textSize">@dimen/text_caption</item>
  </style>

  <style name="TH.Btn_Price_Bar_Separator">
    <item name="android:layout_width">1dp</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:background">@color/tradehero_divider_grey</item>
  </style>

  <style name="TH.ButtonTextShadow">
    <item name="android:shadowRadius">5.6</item>
    <item name="android:shadowDx">0</item>
    <item name="android:shadowDy">-1</item>
    <item name="android:shadowColor">#000000</item>
  </style>

  <style name="TradeHero.Dashboard"
      parent="TradeHero">
  </style>

  <style name="TradeHero.Dashboard.ActionBar.TabText"
      parent="@android:style/Widget.Holo.Light.ActionBar.TabText">
    <item name="android:textColor">@color/tab_text_selector</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textSize">@dimen/text_caption</item>
    <item name="android:textStyle">bold</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="TradeHero.TabText"
      parent="TradeHero.Dashboard.ActionBar.TabText">
    <item name="android:ellipsize">marquee</item>
    <item name="android:maxLines">2</item>
    <item name="android:maxWidth">180dip</item>
  </style>

  <style name="TradeHero.TabView" parent="TradeHero.TabText">
    <item name="android:gravity">center_horizontal</item>
    <item name="android:paddingLeft">16dip</item>
    <item name="android:paddingRight">16dip</item>
    <item name="android:background">@drawable/tab_indicator_ab_th</item>
    <item name="android:layout_width">0dip</item>
    <item name="android:layout_weight">1</item>
  </style>

  <style name="TradeHero.PageTitleView" parent="TradeHero.TabView">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_weight">0</item>
  </style>

  <style name="TradeHero.TabContainer">
    <item name="android:background">?attr/slidingTabBackground</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
  </style>

  <style name="TH.BuySell_ViewPager_Tabs"
      parent="TradeHero.TabView">
    <item name="android:layout_height">match_parent</item>
    <item name="android:gravity">center</item>
    <item name="android:measureWithLargestChild">true</item>
  </style>


  <style name="UserProfile.Tabs" parent="TH.BuySell_ViewPager_Tabs">
  </style>

  <style name="TH.Authentication.EditText"
      parent="@android:style/Widget.EditText">
    <item name="android:drawablePadding">2dp</item>
    <item name="android:padding">2dp</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
  </style>

  <style name="TH.ListItem">
    <item name="android:paddingTop">@dimen/margin_xsmall</item>
    <item name="android:paddingBottom">@dimen/margin_xsmall</item>
    <item name="android:paddingLeft">@dimen/margin_small</item>
    <item name="android:paddingRight">@dimen/margin_small</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:clipToPadding">false</item>
    <item name="android:clipChildren">false</item>
  </style>

  <style name="TH.ListItem.LB">
    <item name="android:paddingTop">0dp</item>
    <item name="android:paddingBottom">0dp</item>
    <item name="android:paddingLeft">0dp</item>
    <item name="android:paddingRight">0dp</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:clipToPadding">false</item>
    <item name="android:clipChildren">false</item>
  </style>

  <style name="TH.ListItem.Clickable" parent="TH.ListItem">
    <item name="android:background">@drawable/basic_white_selector</item>
  </style>

  <style name="TH.ListItem.Clickable.LB" parent="TH.ListItem.LB">
    <item name="android:background">@drawable/basic_white_selector</item>
  </style>

  <style name="TH.VoteView"
      parent="TH.DiscussionActionButtons.Text">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:gravity">center_vertical</item>
  </style>

  <style name="TH.Btn_BuySell_Action" parent="TH.Button.Small.Accented">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:singleLine">true</item>
    <item name="android:ellipsize">middle</item>
    <item name="android:layout_marginLeft">5dp</item>
    <item name="android:layout_marginRight">5dp</item>
    <item name="android:minHeight">@dimen/button_height_small</item>
    <item name="android:maxWidth">180dp</item>
    <item name="android:background">@drawable/basic_white_selector</item>
  </style>

  <style name="TH.SeekBar"
      parent="android:Widget.Holo.Light.SeekBar">
    <item name="android:progressDrawable">@drawable/tradehero_scrubber_progress_horizontal_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/tradehero_scrubber_progress_horizontal_holo_light</item>
    <item name="android:thumb">@drawable/tradehero_scrubber_control_selector_holo_light</item>
  </style>

  <style name="TH.Btn_Invite_Action" parent="TH.Button.Inverse">
    <item name="android:background">@drawable/basic_green_selector</item>
    <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
    <item name="android:paddingTop">5dp</item>
    <item name="android:paddingBottom">5dp</item>
    <item name="android:paddingLeft">10dp</item>
    <item name="android:paddingRight">10dp</item>
    <item name="android:minWidth">80dp</item>
    <item name="android:minHeight">36dp</item>
  </style>

  <style name="TH.Btn_Invite_Action.All" parent="TH.Btn_Invite_Action">
    <item name="android:layout_marginBottom">@dimen/margin_xsmall</item>
  </style>

  <style name="TH.BuySell_ViewPager_Belt"
      parent="TradeHero.TabContainer">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:gravity">center</item>
    <item name="android:divider">?android:attr/dividerVertical</item>
    <item name="android:showDividers">middle</item>
    <item name="android:dividerPadding">6dp</item>
  </style>

  <style name="TH.ShareButtons">
    <item name="android:layout_width">30dp</item>
    <item name="android:layout_height">30dp</item>
    <item name="android:layout_marginRight">5dp</item>
    <item name="android:layout_marginLeft">5dp</item>
    <item name="android:contentDescription">@null</item>
    <item name="android:scaleType">fitCenter</item>
    <item name="android:checked">false</item>
    <item name="android:gravity">center</item>
    <item name="android:textOff">""</item>
    <item name="android:textOn">""</item>
  </style>

  <style name="TH.TextSocialButtons" parent="TH.Subhead.Accented">
    <item name="android:layout_width">32dp</item>
    <item name="android:layout_height">32dp</item>
    <item name="android:layout_marginRight">2dp</item>
    <item name="android:layout_marginLeft">2dp</item>
    <item name="android:checked">false</item>
    <item name="android:gravity">center</item>
    <item name="android:background">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TH.GaugeView">
    <item name="android:layout_height">60dp</item>
    <item name="showText">true</item>
    <item name="showRanges">true</item>
    <item name="rangeColors">@array/gauge_color_range</item>
    <item name="showOuterBorder">false</item>
    <item name="showInnerRim">false</item>
    <item name="showOuterShadow">false</item>
    <item name="showOuterRim">false</item>
    <item name="showNeedle">true</item>
    <item name="gShowText">true</item>
    <item name="animationDuration">4000</item>
    <item name="drawCenterIndicator">false</item>
    <item name="android:layout_marginBottom">10dp</item>
  </style>

  <style name="TH.GaugeView.Small">
    <item name="android:layout_height">30dp</item>
    <item name="android:layout_marginBottom">8dp</item>
  </style>

  <style name="TH.DiscussionActionButtons">
    <item name="android:paddingLeft">@dimen/discussion_action_buttons_horizontal_padding</item>
    <item name="android:paddingRight">@dimen/discussion_action_buttons_horizontal_padding</item>
    <item name="android:paddingTop">@dimen/discussion_action_buttons_vertical_padding</item>
    <item name="android:paddingBottom">@dimen/discussion_action_buttons_vertical_padding</item>
  </style>

  <style name="TH.DiscussionActionButtons.Text"
      parent="TH.DiscussionActionButtons">
    <item name="android:textColor">?android:attr/textColorPrimary</item>
    <item name="android:textSize">@dimen/text_subhead</item>
    <item name="android:layout_gravity">center_vertical</item>
    <item name="android:drawablePadding">@dimen/discussion_action_buttons_drawable_padding</item>
  </style>

  <style name="TH.DiscussionPostButtons"
      parent="TH.Button.Accented">
    <item name="android:gravity">center</item>
    <item name="android:padding">@dimen/discussion_action_buttons_drawable_padding</item>
  </style>

  <style name="TH.Spinner"
      parent="Widget.AppCompat.Light.Spinner.DropDown.ActionBar">
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:listSelector">?android:attr/selectableItemBackground</item>
    <item name="android:dropDownSelector">?android:attr/selectableItemBackground</item>
    <item name="android:background">?android:attr/selectableItemBackground</item>
    <item name="android:theme">@style/ThemeOverlay.AppCompat.Light</item>
  </style>

  <style name="TH.Button.Timeline" parent="TH.Button.Inverse">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:layout_weight">1</item>
    <item name="android:paddingLeft">16dp</item>
    <item name="android:paddingRight">16dp</item>
  </style>

  <style name="TH.Button.Timeline.Follow" parent="TH.Button.Timeline">
    <item name="android:drawableLeft">@drawable/ic_follow</item>
  </style>

  <style name="TH.Button.Timeline.Message" parent="TH.Button.Timeline">
    <item name="android:drawableLeft">@drawable/icn_message_white</item>
  </style>

  <!-- Achievement Styles -->
  <style name="TH.Achievement"
      parent="TH.TextPrimary"/>

  <style name="TH.Achievement.Header"
      parent="TH.Title">
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="TH.Achievement.Title"
      parent="TH.Title">
    <item name="android:gravity">center</item>
  </style>

  <style name="TH.Achievement.Description"
      parent="TH.Body">
    <item name="android:gravity">center</item>
  </style>

  <style name="TH.Achievement.Indicator.Text"
      parent="TH.Achievement.Indicator">
    <item name="android:gravity">center</item>
    <item name="android:textSize">@dimen/text_xxsmall</item>
  </style>

  <style name="TH.Achievement.Indicator.Text.Top"
      parent="TH.Achievement.Indicator.Text">
    <item name="android:gravity">center_horizontal|bottom</item>
  </style>

  <style name="TH.Achievement.Indicator.Text.Bot"
      parent="TH.Achievement.Indicator.Text">
    <item name="android:gravity">center_horizontal|top</item>
  </style>

  <style name="TH.Achievement.Share"
      parent="TH.Button.Inverse">
    <item name="android:textStyle">bold</item>
    <item name="android:minWidth">80dp</item>
    <item name="android:background">@drawable/blue_rounded_button_selector</item>
  </style>

  <style name="TH.Level"
      parent="TH.Caption">
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
  </style>

  <style name="TH.Level.Up.Header"
      parent="TH.Level.Up">
    <item name="android:textSize">@dimen/text_headline</item>
  </style>

  <style name="TH.Level.Up.Desc"
      parent="TH.Level.Up">
    <item name="android:textSize">@dimen/text_title</item>
  </style>

  <style name="TH.Achievement.Earned"
      parent="TH.Title">
    <item name="android:gravity">center</item>
  </style>

  <style name="TH.Achievement.Earned.VirtualDollar"
      parent="TH.Achievement.Earned">
    <item name="android:textColor">#F3A422</item>
  </style>

  <style name="TH.Achievement.Earned.XP"
      parent="TH.Achievement.Earned">
    <item name="android:textColor">#407304</item>
  </style>

  <style name="TH.Achievement.Earned.LevelUp"
      parent="TH.Body">
    <item name="android:textColor">#CE021B</item>
  </style>

  <style name="TH.Achievement.Pulse">
    <item name="android:layout_centerHorizontal">true</item>
    <item name="android:layout_alignParentTop">true</item>
    <item name="android:layout_marginTop">@dimen/achievement_pulse_circle_margin_top</item>
    <item name="android:contentDescription">@null</item>
    <item name="android:layerType">hardware</item>
    <item name="android:drawingCacheQuality">low</item>
    <item name="android:visibility">gone</item>
  </style>

  <style name="TH.Achievement.Indicator"
      parent="TH.Achievement">
    <item name="android:tag">@string/tag_achievement_indicator</item>
    <item name="android:layout_marginLeft">3dp</item>
    <item name="android:layout_marginRight">3dp</item>
  </style>

  <style name="TH.Achievement.Indicator.Image"
      parent="TH.Achievement.Indicator">
    <item name="android:scaleType">fitCenter</item>
    <item name="android:padding">@dimen/achievement_indicator_star_padding</item>
    <item name="android:background">@drawable/achievement_indicator_bg_padded</item>
    <item name="android:src">@drawable/ic_achievement_star_off</item>
  </style>

  <style name="TH.Level.Current"
      parent="TH.Level"/>

  <style name="TH.Level.Next"
      parent="TH.Level">
    <item name="android:alpha">0.7</item>
  </style>

  <style name="TH.Level.Up"
      parent="TH.Level">
    <item name="android:shadowDy">1</item>
    <item name="android:shadowDx">1</item>
    <item name="android:shadowRadius">1</item>
    <item name="android:shadowColor">@color/black</item>
    <item name="android:textColor">@color/white</item>
  </style>

  <style name="TH.ButtonClose"
      parent="android:Widget.Holo.Button.Small">
    <item name="android:textColor">@color/black</item>
  </style>

  <style name="TH.Level.ProgressBar"
      parent="android:Widget.Holo.ProgressBar.Horizontal">
    <item name="android:progressDrawable">@drawable/user_level_progress</item>
    <item name="android:minHeight">@dimen/user_level_progressbar_height</item>
    <item name="android:maxHeight">@dimen/user_level_progressbar_height</item>
  </style>

  <style name="TH.Achievement.Dialog"
      parent="android:Theme.Holo.Light.Dialog">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowFullscreen">true</item>
    <item name="android:windowIsFloating">false</item>
    <item name="android:windowBackground">@null</item>
  </style>

  <style name="TH.Achievement.Dialog.Animation"
      parent="@android:style/Animation.Activity">
    <item name="android:windowEnterAnimation">@anim/achievement_zoom_in</item>
    <item name="android:windowExitAnimation">@anim/achievement_dismiss_left</item>
  </style>

  <style name="TH.Achievement.Dialog.NoAnimation"
      parent="@android:style/Animation.Activity">
    <item name="android:windowEnterAnimation">@null</item>
    <item name="android:windowExitAnimation">@null</item>
  </style>

  <style name="TH.Achievement.QuestIndicator">
    <item name="android:tag">@string/tag_quest_indicator</item>
    <item name="android:layout_gravity">center_vertical</item>
  </style>

  <!-- End of Achievements Styles -->

  <style name="TH.StockAlert.Dialog"
      parent="android:Theme.Holo.Light.Dialog">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsFloating">false</item>
    <item name="android:windowBackground">@null</item>
  </style>

  <style name="TH.Btn_Popup_Close">
    <item name="android:src">@drawable/ic_btn_popup_close_blue</item>
    <item name="android:minWidth">@dimen/clickable_element_min_dimen</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:layout_width">@dimen/clickable_element_min_dimen</item>
    <item name="android:layout_height">@dimen/clickable_element_min_dimen</item>
    <item name="android:scaleType">centerInside</item>
    <item name="android:background">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TH.Btn_Popup_Close.Inverse">
    <item name="android:src">@drawable/ic_btn_popup_close_white</item>
  </style>

  <style name="TH.RatingBarSmall"
      parent="android:Widget.Holo.Light.RatingBar.Small">
    <item name="android:progressDrawable">@drawable/th_ratingbar_small</item>
    <item name="android:indeterminateDrawable">@drawable/th_ratingbar_small</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.TabView"
      parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
    <item name="android:background">@drawable/tab_indicator_ab_th</item>
  </style>


  <style name="TH.ContentPartnersLogo">
    <item name="android:minWidth">@dimen/size_7</item>
    <item name="android:minHeight">@dimen/size_4</item>
    <item name="android:scaleType">fitCenter</item>
    <item name="android:padding">@dimen/content_partner_padding</item>
    <item name="android:alpha">1.0</item>
  </style>

  <style name="TH.LeftDrawerMenu">
    <item name="android:alpha">1</item>
  </style>

  <style name="TH.LeftDrawerMenu.Container">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:orientation">horizontal</item>
    <item name="android:paddingLeft">?attr/listPreferredItemPaddingLeft</item>
    <item name="android:paddingRight">?attr/listPreferredItemPaddingRight</item>
    <item name="android:gravity">center_vertical</item>
    <item name="android:background">?attr/selectableItemBackground</item>
    <item name="android:clickable">true</item>
    <item name="android:focusable">true</item>
  </style>

  <style name="TH.LeftDrawerMenu.Text"
      parent="TH.Subhead.Inverse">
    <item name="android:layout_marginLeft">@dimen/margin_large</item>
  </style>

  <style name="TH.LeftDrawerMenu.Icon"
      parent="TH.LeftDrawerMenu">
    <item name="android:layout_width">@dimen/size_3</item>
    <item name="android:layout_height">@dimen/size_3</item>
    <item name="android:scaleType">fitCenter</item>
  </style>

  <style name="TH.Dialog"
      parent="android:Theme.Holo.Light.Dialog">
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    <item name="android:windowFrame">@null</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowAnimationStyle">@null</item>
    <item name="android:windowIsFloating">true</item>
    <item name="android:backgroundDimEnabled">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowCloseOnTouchOutside">true</item>
  </style>

