  <style name="TradeHero.Dashboard.ActionBar.TabBar"
      parent="@android:style/Widget.Holo.Light.ActionBar.TabBar">
    <item name="android:background">@android:color/white</item>
  </style>

  <style name="TradeHero.Dashboard.ActionBar.TabText"
      parent="@android:style/Widget.Holo.Light.ActionBar.TabText">
    <item name="android:textColor">?android:textColorPrimaryInverse</item>
    <item name="android:textAllCaps">true</item>
    <item name="android:textSize">@dimen/text_caption</item>
    <item name="android:textStyle">bold</item>
    <item name="android:gravity">center</item>
  </style>

  <style name="TradeHero.TabText"
      parent="TradeHero.Dashboard.ActionBar.TabText">
    <item name="android:ellipsize">marquee</item>
    <item name="android:maxLines">2</item>
    <item name="android:maxWidth">180dip</item>
  </style>

  <style name="TradeHero.TabView" parent="TradeHero.TabText">
    <item name="android:gravity">center_horizontal</item>
    <item name="android:paddingLeft">16dip</item>
    <item name="android:paddingRight">16dip</item>
    <item name="android:background">@drawable/tab_indicator_ab_th</item>
    <item name="android:layout_width">0dip</item>
    <item name="android:layout_weight">1</item>
    <item name="android:minWidth">80dip</item>
  </style>

  <style name="TradeHero.PageTitleView" parent="TradeHero.TabView">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_weight">0</item>
  </style>

  <style name="TradeHero.TabContainer">
    <item name="android:background">@drawable/bar_background</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
  </style>

  <style name="TH.BuySell_ViewPager_Tabs"
      parent="TradeHero.TabView">
    <item name="android:layout_height">match_parent</item>
    <item name="android:gravity">center</item>
    <item name="android:measureWithLargestChild">true</item>
  </style>


  <style name="UserProfile.Tabs" parent="TH.BuySell_ViewPager_Tabs">
  </style>

