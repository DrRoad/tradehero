<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <!--
      Base application theme, dependent on API level. This theme is replaced
      by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
  -->

  <style name="Base.TradeHero"
      parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorAccent">@color/text_secondary_accented</item>
    <item name="colorPrimary">@color/tradehero_blue</item>
    <item name="colorPrimaryDark">#ff0812c6</item>
    <item name="colorControlNormal">@color/tradehero_blue</item>
    <item name="colorControlHighlight">@color/tradehero_blue_pressed</item>
    <item name="android:textColorPrimary">@color/text_primary</item>
    <item name="android:textColorPrimaryInverse">@color/text_primary_inverse</item>
    <item name="android:textColorSecondary">@color/text_secondary</item>
    <item name="android:textColorSecondaryInverse">@color/text_secondary_inverse</item>
    <item name="android:textColorHighlight">@color/text_primary_accented</item>
    <item name="android:textColorHighlightInverse">@color/text_secondary_accented</item>
  </style>

  <style name="TradeHero"
      parent="Base.TradeHero">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowActionBarOverlay">false</item>
  </style>

  <style name="TradeHero.ActionBar" parent="Widget.AppCompat.Toolbar">
    <item name="android:background">?attr/colorPrimary</item>
    <item name="theme">@style/ThemeOverlay.ActionBar.TradeHero</item>
    <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
  </style>

  <style name="ThemeOverlay.ActionBar.TradeHero" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
    <item name="android:windowActionBarOverlay">true</item>
    <!-- Support library compatibility -->
    <item name="windowActionBarOverlay">true</item>
  </style>

  <style name="TradeHero.Settings"
      parent="TradeHero">
  </style>

  <style name="AppBaseTheme.NoActionBar"
      parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:background">@color/white</item>
  </style>

  <style name="AppBaseTheme.NoActionBar2"
      parent="TradeHero">
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowBackground">@drawable/authentication_bg</item>
  </style>

  <style name="TH"/>

  <style name="TH.Btn_Bar_Base"
      parent="TH.Button">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:minHeight">@dimen/clickable_element_min_dimen</item>
    <item name="android:gravity">center</item>
    <item name="android:textStyle">bold</item>
    <item name="android:background">@drawable/basic_transparent_selector</item>
  </style>

  <style name="TH.Btn_Buy_Sell"
      parent="TH.Btn_Bar_Base">
    <item name="android:background">@drawable/blue_rounded_button_selector</item>
    <item name="android:textColor">@color/text_primary_inverse</item>
  </style>

  <style name="TH.Btn_Time_Span"
      parent="TH.Btn_Bar_Base">
    <item name="android:textColor">@color/text_secondary</item>
    <item name="android:textSize">@dimen/text_caption</item>
    <item name="android:minHeight">@dimen/timespan_button_min_height</item>
  </style>

  <style name="TH.BtnQuickPriceSet"
      parent="TH.Btn_Bar_Base">
    <item name="android:textColor">@color/text_secondary</item>
    <item name="android:textSize">@dimen/text_caption</item>
  </style>

  <style name="TH.Btn_Price_Bar_Separator">
    <item name="android:layout_width">1dp</item>
    <item name="android:layout_height">match_parent</item>
    <item name="android:background">@color/tradehero_divider_grey</item>
  </style>

  <style name="TH.Authentication">
    <item name="android:editTextStyle">@style/TH.Authentication.EditText</item>
    <item name="android:paddingTop">@dimen/margin_large</item>
    <item name="android:paddingRight">@dimen/margin_small</item>
    <item name="android:paddingLeft">@dimen/margin_small</item>
    <item name="android:paddingBottom">@dimen/margin_small</item>
  </style>

  <style name="TH.ButtonTextShadow">
    <item name="android:shadowRadius">5.6</item>
    <item name="android:shadowDx">0</item>
    <item name="android:shadowDy">-1</item>
    <item name="android:shadowColor">#000000</item>
  </style>

  <style name="TH.SocialAuthenticationButton">
    <item name="android:textColor">@color/white</item>
    <item name="android:textSize">@dimen/authentication_button_text_size</item>
    <item name="android:textStyle">bold</item>
    <item name="android:layout_marginTop">@dimen/authentication_button_vertical_margin</item>
    <item name="android:layout_marginBottom">@dimen/authentication_button_vertical_margin</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:gravity">left|center_vertical</item>
    <item name="android:drawablePadding">@dimen/authentication_button_drawable_padding</item>
    <item name="android:paddingLeft">@dimen/authentication_button_horizontal_padding</item>
    <item name="android:paddingRight">@dimen/authentication_button_horizontal_padding</item>
  </style>

