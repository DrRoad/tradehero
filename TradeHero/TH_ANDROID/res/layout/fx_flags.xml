<?xml version="1.0" encoding="utf-8"?>
<com.tradehero.th.fragments.security.FxFlagContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flags_container"
    android:layout_width="@dimen/big_image_w_h"
    android:layout_height="wrap_content">
    <!--android:layout_height="@dimen/big_image_w_h">-->

  <ImageView
      android:id="@+id/flag_right"
      android:layout_gravity="center_vertical|right"
      android:layout_width="@dimen/small_image_w_h"
      android:layout_height="@dimen/small_image_w_h"
      tools:src="@drawable/square_de"/>

  <ImageView
      android:id="@+id/flag_left"
      android:layout_gravity="center_vertical|left"
      android:layout_width="@dimen/small_image_w_h"
      android:layout_height="@dimen/small_image_w_h"
      tools:src="@drawable/square_fr"/>

</com.tradehero.th.fragments.security.FxFlagContainer>