<?xml version="1.0" encoding="utf-8"?>

<com.tradehero.th.fragments.competition.CompetitionZoneLegalMentionsView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.tradehero.th"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="8dp"
    android:background="@color/white"
    >

  <TextView
      android:id="@+id/competition_legal_rules"
      android:layout_width="wrap_content"
      android:layout_height="0dp"
      android:maxLines="1"
      />

  <TextView
      android:id="@+id/competition_legal_terms"
      android:layout_width="wrap_content"
      android:layout_height="0dp"
      android:layout_marginLeft="8dp"
      />

</com.tradehero.th.fragments.competition.CompetitionZoneLegalMentionsView>