<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <Button
      android:id="@+id/btn_weibo_signin"
      style="@style/TradeHero.SocialAuthenticationButton"
      android:drawableLeft="@drawable/settings_weibo_normal"
      android:background="@drawable/btn_background_social_weibo"
      android:text="@string/weibo_login_in"/>

  <Button
      android:id="@+id/btn_qq_signin"
      style="@style/TradeHero.SocialAuthenticationButton"
      android:drawableLeft="@drawable/settings_qq_normal"
      android:background="@drawable/btn_background_social_qq"
      android:text="@string/qq_login_in"/>
</merge>